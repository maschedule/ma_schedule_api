<%= simple_form_for @period do |f| %>
  <%= f.input :production, placeholder: 'Production' %>
  <%= f.input :city, placeholder: 'City' %>
  <%= f.input :place, placeholder: 'Place' %>
  <%= f.input :from %>
  <%= f.input :to %>
  <%= f.input :notes, as: :text, placeholder: 'Notes' %>
  <%= f.button :submit %>
<% end %>